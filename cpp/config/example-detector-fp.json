[
	{
		"fr_setup": {
			"fr_ready_cnxn": "tcp://127.0.0.1:5001",
			"fr_release_cnxn": "tcp://127.0.0.1:5002"
		},
		"meta_endpoint": "tcp://*:5008"
	},
	{
		"plugin": {
			"load": {
				"index": "hdf",
				"name": "FileWriterPlugin",
				"library": "${CMAKE_INSTALL_PREFIX}/lib/libHdf5Plugin.so"
			}
		}
	},
	{
		"plugin": {
			"load": {
				"index": "example",
				"name": "ExampleDetectorPlugin",
				"library": "${CMAKE_INSTALL_PREFIX}/lib/libExampleDetectorPlugin.so"
			}
		}
	},
	{
		"plugin": {
			"connect": {
				"index": "example",
				"connection": "frame_receiver"
			}
		}
	},
	{
		"plugin": {
			"connect": {
				"index": "hdf",
				"connection": "example"
			}
		}
	},
	{
		"hdf": {
			"dataset": {
				"example": {
					"datatype": "uint8",
					"dims": [
						256,
						256
					],
					"compression": "none"
				}
			},
			"file": {
				"path": "/tmp",
				"first_number": 0
			},
			"timeout_timer_period": 3000
		}
	}
]
